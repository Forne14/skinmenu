  "bugs": {
    "url": "https://github.com/Forne14/skinmenu/issues"
  },
  "homepage": "https://github.com/Forne14/skinmenu#readme",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.17",
    "@tailwindcss/typography": "^0.5.19"
  }
}
