{
  "name": "skinmenu",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=18 <23"
  },
  "scripts": {
    "css:build": "tailwindcss -c ./tailwind.config.js -i ./src/styles/tailwind.css -o ./config/static/css/app.css --minify",
    "css:watch": "tailwindcss -c ./tailwind.config.js -i ./src/styles/tailwind.css -o ./config/static/css/app.css --watch",
    "css:dev": "tailwindcss -c ./tailwind.config.js -i ./src/styles/tailwind.css -o ./config/static/css/app.css --watch --sourcemap",
    "verify:css": "node -v && npm -v && tailwindcss --version && npm run css:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forne14/skinmenu.git"
  },
